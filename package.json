{
  "name": "esport-hatcher",
  "version": "1.0.0",
  "description": "eip api",
  "main": "build/server.js",
  "repository": "https://github.com/agamblin/esport-hatcher",
  "author": "agamblin",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "start": "node build/server.js",
    "serve": "concurrently --kill-others \"npm run build:watch\" \"nodemon --max-old-space-size=6144 build/server.js\"",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "lint": "tslint -t stylish --project tsconfig.json --config tslint.json",
    "launch-tests": "jest -i",
    "test": "sh launch-tests.sh",
    "test:watch": "jest --watch",
    "shell": "sh launch-shell.sh",
    "seed": "sh launch-seeding.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config .prettierrc.json --write",
      "tslint -t stylish --project tsconfig.json --config tslint.json"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.576.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jwt-simple": "^0.5.5",
    "lint-staged": "^10.1.7",
    "lodash": "^4.17.11",
    "module-alias": "^2.2.0",
    "mysql2": "^2.1.0",
    "node": "^14.0.0",
    "nodemon": "^2.0.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "query-string": "^6.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "5.21.7",
    "superagent": "^5.0.2",
    "supertest": "^4.0.2",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.26",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.16.1",
    "@types/faker": "^4.1.5",
    "@types/jest": "^25.2.1",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.121",
    "@types/node": "^13.13.4",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.27.49",
    "@types/supertest": "^2.0.7",
    "@types/validator": "^13.0.0",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.8.3"
  },
  "_moduleAliases": {
    "@controllers": "build/lib/controllers",
    "@factories": "build/lib/factories",
    "@middlewares": "build/lib/middlewares",
    "@routes": "build/lib/routes",
    "@services": "build/lib/services",
    "@models": "build/db/models",
    "@utils": "build/lib/utils",
    "@config": "build/config",
    "@tests": "build/tests",
    "@app": "build/lib/app.js",
    "@db": "build/db/index.js"
  }
}
